{
  "sourceMaps": "inline",
  "presets": [
    ["es2015", { "loose": true, "modules": "commonjs" }],
    ["es2017"],
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["babel-plugin-transform-builtin-extend", {
        globals: ["Error"]
    }]
  ]
}
