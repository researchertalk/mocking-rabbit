{
  "name": "mocking-rabbit",
  "version": "0.0.1",
  "author": "gianfebrian<gianfebrian@gmail.com>",
  "description": "We play with alice, we found no bird only rabbit. An amqplib mock.",
  "keywords": [
    "alice",
    "rabbit",
    "mq",
    "amqplib",
    "amqp"
  ],
  "main": "index.js",
  "scripts": {
    "babel": "babel -D src/ --out-dir lib/ --source-maps inline",
    "coveralls": "cat ./test/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "babel -w -D src/ --out-dir lib/ --source-maps inline",
    "lint": "eslint src/**",
    "prepublish": "npm run babel",
    "test": "npm run lint && npm run babel && istanbul --config=test/.istanbul.yml cover node_modules/mocha/bin/_mocha -- --check-leaks -b -R spec test/index.js"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "lodash": "^4.14.1",
    "semver": "^5.3.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.11.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.12.0",
    "faker": "^3.1.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
